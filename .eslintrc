{
  "extends":"react-app",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "env": {
    "es6": true
  },
  "rules": {
    "indent": ["error", 2],
    "semi": ["error", "always"],
    "no-unused-vars": "error",
    "quotes": ["error", "double"],
    "no-use-before-define": "error",
    "camelcase": "error",
    "no-var": "error",
    "no-trailing-spaces": "error",
    "no-multi-spaces": "error",
    "no-unreachable": "error" 
  }
}

